{
  "name": "nodejs-loadbalance-stats",
  "version": "1.0.0",
  "description": "a visual display for statistics from a nodeJS logger",
  "scripts": {
    "web.start": "webpack-dev-server --inline --progress --port 8080",
    "web.test": "karma start",
    "web.build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "Jake Nesom",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "~4.0.1",
    "@angular/http": "~4.0.1",
    "@angular/platform-browser": "~4.0.1",
    "@angular/platform-browser-dynamic": "~4.0.1",
    "@angular/router": "~4.0.1",
    "angular-in-memory-web-api": "~0.3.1",
    "angular2-highcharts": "^0.5.5",
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "chart.js": "^2.5.0",
    "connect-cors": "^0.5.6",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "ng-fx": "^2.0.4",
    "ng2-charts": "^1.5.0",
    "rxjs": "5.2.0",
    "systemjs": "0.19.47",
    "vis": "^4.18.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.46",
    "@types/node": "^7.0.12",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "null-loader": "^0.1.1",
    "protractor": "~5.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "repository": {}
}
